  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"config/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "pp": "TZ=UTC NODE_ENV=pp nest start",
    "prod": "TZ=UTC NODE_ENV=prod nest start",
    "local": "TZ=UTC NODE_ENV=local nest start --debug --watch",
    "dev": "TZ=UTC NODE_ENV=dev nest start --debug --watch",
    "qa": "NODE_ENV=qa nest start --debug --watch",
    "stg": "NODE_ENV=stg nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:build:dev" : "NODE_ENV=dev nest start && exit"
  },
